{
  "name": "vue-flipt",
  "version": "0.1.0",
  "description": "Vue client plugin for Flipt",
  "author": "Mauro Verrocchio <mauroverrocchio@gmail.com>",
  "license": "ISC",
  "keywords": [
    "vue",
    "vuejs",
    "directive",
    "flipt",
    "feature flag"
  ],
  "repository": {
    "type": "git",
    "url": "https://https://github.com/maur8ino/vue-flipt"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-flipt ./src/plugin.js",
    "test": "yarn test:unit",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vue-flipt.common.js",
  "unpkg": "dist/vue-flipt.umd.min.js",
  "files": [
    "dist/*",
    "src/*",
    "*.json"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "axios": "^0.21.0",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "jsdelivr": "dist/vue-flipt.umd.min.js",
  "sideeffects": false
}
